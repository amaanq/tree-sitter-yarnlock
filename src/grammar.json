{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "yarnlock",
  "rules": {
    "lock_file": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "dependency"
      }
    },
    "dependency": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "FIELD",
                          "name": "name",
                          "content": {
                            "type": "SYMBOL",
                            "name": "identifier"
                          }
                        },
                        {
                          "type": "SEQ",
                          "members": [
                            {
                              "type": "STRING",
                              "value": "\""
                            },
                            {
                              "type": "FIELD",
                              "name": "name",
                              "content": {
                                "type": "SYMBOL",
                                "name": "identifier"
                              }
                            },
                            {
                              "type": "STRING",
                              "value": "\""
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "STRING",
                      "value": "@"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "semver"
                    }
                  ]
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "CHOICE",
                            "members": [
                              {
                                "type": "FIELD",
                                "name": "name",
                                "content": {
                                  "type": "SYMBOL",
                                  "name": "identifier"
                                }
                              },
                              {
                                "type": "SEQ",
                                "members": [
                                  {
                                    "type": "STRING",
                                    "value": "\""
                                  },
                                  {
                                    "type": "FIELD",
                                    "name": "name",
                                    "content": {
                                      "type": "SYMBOL",
                                      "name": "identifier"
                                    }
                                  },
                                  {
                                    "type": "STRING",
                                    "value": "\""
                                  }
                                ]
                              }
                            ]
                          },
                          {
                            "type": "STRING",
                            "value": "@"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "semver"
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "\""
                    },
                    {
                      "type": "FIELD",
                      "name": "name",
                      "content": {
                        "type": "ALIAS",
                        "content": {
                          "type": "PATTERN",
                          "value": "@?[^@]+"
                        },
                        "named": true,
                        "value": "url"
                      }
                    },
                    {
                      "type": "STRING",
                      "value": "@"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "semver"
                    },
                    {
                      "type": "STRING",
                      "value": "\""
                    }
                  ]
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "STRING",
                            "value": "\""
                          },
                          {
                            "type": "FIELD",
                            "name": "name",
                            "content": {
                              "type": "ALIAS",
                              "content": {
                                "type": "PATTERN",
                                "value": "@?[^@]+"
                              },
                              "named": true,
                              "value": "url"
                            }
                          },
                          {
                            "type": "STRING",
                            "value": "@"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "semver"
                          },
                          {
                            "type": "STRING",
                            "value": "\""
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "SYMBOL",
          "name": "dependency_body"
        }
      ]
    },
    "dependency_body": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "version"
          },
          {
            "type": "SYMBOL",
            "name": "resolved"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "dependencies"
                },
                {
                  "type": "SYMBOL",
                  "name": "integrity"
                }
              ]
            }
          }
        ]
      }
    },
    "version": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "version"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "\""
                },
                {
                  "type": "STRING",
                  "value": "version"
                },
                {
                  "type": "STRING",
                  "value": "\""
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "SYMBOL",
          "name": "semver"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "resolved": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "resolved"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "\""
                },
                {
                  "type": "STRING",
                  "value": "resolved"
                },
                {
                  "type": "STRING",
                  "value": "\""
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[^\"]+"
          },
          "named": true,
          "value": "url"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "dependencies": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "dependencies"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "\""
                },
                {
                  "type": "STRING",
                  "value": "dependencies"
                },
                {
                  "type": "STRING",
                  "value": "\""
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "SYMBOL",
          "name": "dependency_list"
        }
      ]
    },
    "dependency_list": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "dependency_entry"
      }
    },
    "dependency_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "\""
                },
                {
                  "type": "SYMBOL",
                  "name": "identifier"
                },
                {
                  "type": "STRING",
                  "value": "\""
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "SYMBOL",
          "name": "semver"
        },
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "integrity": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "integrity"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "\""
                },
                {
                  "type": "STRING",
                  "value": "integrity"
                },
                {
                  "type": "STRING",
                  "value": "\""
                }
              ]
            }
          ]
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[^\\s].+"
          },
          "named": true,
          "value": "hash"
        }
      ]
    },
    "semver": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "operator",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "^"
                  },
                  {
                    "type": "STRING",
                    "value": "~"
                  },
                  {
                    "type": "STRING",
                    "value": "="
                  },
                  {
                    "type": "STRING",
                    "value": ">="
                  },
                  {
                    "type": "STRING",
                    "value": ">"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[0-9]+"
          },
          "named": true,
          "value": "major"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "IMMEDIATE_TOKEN",
                  "content": {
                    "type": "STRING",
                    "value": "."
                  }
                },
                {
                  "type": "ALIAS",
                  "content": {
                    "type": "PATTERN",
                    "value": "[0-9]+"
                  },
                  "named": true,
                  "value": "minor"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "IMMEDIATE_TOKEN",
                          "content": {
                            "type": "STRING",
                            "value": "."
                          }
                        },
                        {
                          "type": "ALIAS",
                          "content": {
                            "type": "PATTERN",
                            "value": "(x|[0-9]+)"
                          },
                          "named": true,
                          "value": "patch"
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "prerelease",
              "content": {
                "type": "PATTERN",
                "value": "\\-[^:\"\\s]+"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "build",
              "content": {
                "type": "PATTERN",
                "value": "\\+[^:\"\\s]+"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_indent": {
      "type": "PATTERN",
      "value": "  "
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "@?[.a-zA-Z0-9_/-]+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "dependency_list"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
